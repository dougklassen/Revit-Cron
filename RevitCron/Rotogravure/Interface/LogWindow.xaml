<Window x:Class="DougKlassen.Revit.Cron.Rotogravure.Interface.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rotogravure Log" Height="700" Width="600">
    <DockPanel LastChildFill="True">
        <Canvas DockPanel.Dock="Bottom" Height="40">
            <Button x:Name="OkButton" Content="OK" Height="27" Width="60" Canvas.Top="7" Canvas.Right="9" Click="OkButton_Click"/>
        </Canvas>
        <ScrollViewer>
            <TextBox x:Name="LogViewTextBox" TextWrapping="Wrap" />
        </ScrollViewer>
    </DockPanel>
</Window>
